{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "NEST_CLI_SRC", // REMPLACER PAR LE CHEMIN VERS LE DOSSIER 
    "compilerOptions": {
      "plugins": [
        {
          "name": "@nestjs/swagger",
          "options": {
            "classValidatorShims": false,
            "introspectComments": true
          }
        }
      ]
    },
    "root": "/app/src"
  }
  